<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.EXAMPLE.pastpath</string>

  <key>ProgramArguments</key>
  <array>
    <string>/usr/local/pastpath/pastpath</string>
  </array>

  <key>RunAtLoad</key>
  <true/>

  <key>KeepAlive</key>
  <true/>

  <key>WorkingDirectory</key>
  <string>/usr/local/pastpath</string>

  <key>StandardOutPath</key>
  <string>/usr/local/pastpath/pastpath.log</string>

  <key>StandardErrorPath</key>
  <string>/usr/local/pastpath/pastpath.error.log</string>
</dict>
</plist>

